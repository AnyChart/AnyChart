<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://anychart.com/schemas/7.10.0/xml-schema.xsd"
        xmlns="http://anychart.com/schemas/7.10.0/xml-schema.xsd"
        elementFormDefault="qualified">

    <xsd:element name="anychart" type="Anychart"/>

    <xsd:complexType name="Anychart">
        <xsd:choice minOccurs="0" maxOccurs="1">
            <xsd:element name="chart" type="Chart"/>
            <xsd:element name="gauge" type="Gauge"/>
            <xsd:element name="gantt" type="Gantt"/>
            <xsd:element name="map" type="Map"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="Chart">
        <xsd:all>
            <!-- VisualBaseWithBounds elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="bounds" type="utils.Bounds"/>
            <!-- core.Chart elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="title" type="core.Title"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="background" type="core.Background"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="margin" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="padding" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="legend" type="core.Legend"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="credits" type="core.Credits"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="chart_labels" type="core.Label_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="animation" type="utils.Animation"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="interactivity" type="utils.Interactivity"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="context_menu" type="utils.ContextMenu"/>
            <!-- Common elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="data" type="Data"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="tree_data" type="TreeData"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="palette" type="palettes.Colors"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill_palette" type="palettes.HatchFills"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="marker_palette" type="palettes.Markers"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="grids" type="Grid_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="minor_grids" type="Grid_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="scales" type="Scale_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="x_scale" type="Scale"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="y_scale" type="Scale"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="series_list" type="Series_list"/>
            <!-- charts.Bullet elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="range_palette" type="palettes.Colors"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="scale" type="Scale"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="axis" type="core.axes.Linear"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="ranges" type="core.axisMarkers.SimpleRange_list"/>
            <!-- charts.Cartesian and chart.Scatter elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="crosshair" type="core.Crosshair"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="x_axes" type="core.axes.Linear_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="y_axes" type="core.axes.Linear_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="line_axes_markers" type="core.axisMarkers.Line_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="range_axes_markers" type="core.axisMarkers.Range_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="text_axes_markers" type="core.axisMarkers.Text_list"/>
            <!-- charts.Polar and charts.Radar elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="x_axis" type="core.axes.RadarPolar"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="y_axis" type="core.axes.Radial"/>
            <!-- charts.Pie and charts.Funnel and charts.Pyramid elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hover_labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="tooltip" type="core.TooltipOrChartTooltip"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="connector_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hover_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hover_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hover_hatch_fill" type="HatchFill"/>
            <!-- charts.Funnel and charts.Pyramid elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="hover_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="select_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="select_labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="select_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="select_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="select_hatch_fill" type="HatchFill"/>
            <!-- charts.Sparkline elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="clip" type="utils.Rect"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="last_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="last_hatch_fill" type="HatchFill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="last_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="last_labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="first_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="first_hatch_fill" type="HatchFill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="first_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="first_labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="max_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="max_hatch_fill" type="HatchFill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="max_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="max_labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="min_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="min_hatch_fill" type="HatchFill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="min_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="min_labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="negative_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="negative_hatch_fill" type="HatchFill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="negative_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="negative_labels" type="core.LabelsFactory"/>
            <!-- charts.Sparkline and charts.Funnel and charts.Pyramid elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="markers" type="core.MarkersFactory"/>
            <!-- charts.HeatMap elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="color_scale" type="Scale"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="y_zoom" type="core.CartesianXZoom"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="y_scroller" type="core.ChartScroller"/>
            <!-- charts.TreeMap elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="headers" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hover_headers" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="color_range" type="core.ColorRange"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="drill_to" type="DrillToList"/>
            <!-- new Cartesian elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="x_zoom" type="core.CartesianXZoom"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="x_scroller" type="core.ChartScroller"/>
        </xsd:all>
        <!-- core.Chart attrs -->
        <xsd:attributeGroup ref="attrs.VisualBase"/>
        <xsd:attributeGroup ref="attrs.Bounds"/>
        <xsd:attribute name="type" type="enums.ChartTypes" use="required"/>
        <xsd:attribute name="default_series_type" type="xsd:string"/>
        <xsd:attribute name="container" type="xsd:string"/>
        <xsd:attribute name="title" type="simple.StringOrBoolOrNull" default="true"/>
        <xsd:attribute name="background" type="simple.StringOrBoolOrNull" default="false"/>
        <xsd:attribute name="animation" type="simple.Bool" default="false"/>
        <xsd:attribute name="context_menu" type="simple.Bool" default="true"/>
        <!-- charts.Bullet attrs -->
        <xsd:attribute name="layout" type="enums.Layout" default="horizontal"/>
        <xsd:attribute name="scale" type="simple.ChartScaleRef" default="linear"/>
        <xsd:attribute name="axis" type="simple.BoolOrNull" default="true"/>
        <!-- charts.Cartesian attrs -->
        <xsd:attribute name="bar_chart_mode" type="simple.Bool" default="false"/>
        <xsd:attribute name="bar_groups_padding" type="xsd:double" default="0.5"/>
        <xsd:attribute name="bars_padding" type="xsd:double" default="0.1"/>
        <xsd:attribute name="min_bubble_size" type="simple.NumberOrPercent" default="5%"/>
        <xsd:attribute name="max_bubble_size" type="simple.NumberOrPercent" default="20%"/>
        <xsd:attribute name="x_scale" type="simple.ChartScaleRef"/>
        <xsd:attribute name="y_scale" type="simple.ChartScaleRef"/>
        <!-- charts.Cartesian3d attrs -->
        <xsd:attribute name="z_angle" type="xsd:double" default="45"/>
        <xsd:attribute name="z_aspect" type="simple.NumberOrPercent" default="50%"/>
        <xsd:attribute name="z_depth" type="simple.NumberOrNull"/>
        <xsd:attribute name="z_padding" type="xsd:double" default="10"/>
        <xsd:attribute name="z_distribution" type="xsd:boolean" default="false"/>
        <!-- charts.Funnel attrs -->
        <xsd:attribute name="neck_height" type="simple.NumberOrPercent" default="25%"/>
        <xsd:attribute name="neck_width" type="simple.NumberOrPercent" default="25%"/>
        <!-- charts.Funnel and charts.Pyramid attrs -->
        <xsd:attribute name="base_width" type="simple.NumberOrPercent" default="90%"/>
        <xsd:attribute name="points_padding" type="simple.NumberOrPercent" default="5"/>
        <xsd:attribute name="hover_markers" type="simple.MarkerType"/>
        <xsd:attribute name="select_markers" type="simple.MarkerType"/>
        <xsd:attribute name="select_labels" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="select_fill" type="xsd:string"/>
        <xsd:attribute name="select_stroke" type="xsd:string"/>
        <xsd:attribute name="select_hatch_fill" type="simple.HatchFill" default="false"/>
        <!-- chart.Polar and charts.Radar common attrs -->
        <xsd:attribute name="start_angle" type="xsd:double" default="0"/>
        <!-- also for pie -->
        <xsd:attribute name="x_axis" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="y_axis" type="simple.BoolOrNull" default="true"/>
        <!-- charts.Pie attrs -->
        <!--<xsd:attribute name="group" type="xsd:boolean"/> NO SUPPORT IN XML YET-->
        <xsd:attribute name="labels" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="hover_labels" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="tooltip" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="sort" type="enums.Sort" default="none"/>
        <xsd:attribute name="radius" type="simple.NumberOrPercent" default="100%"/>
        <xsd:attribute name="inner_radius" type="simple.NumberOrPercent" default="0"/>
        <xsd:attribute name="explode" type="simple.NumberOrPercent" default="15"/>
        <xsd:attribute name="outside_labels_space" type="simple.NumberOrPercent" default="30%"/>
        <xsd:attribute name="inside_labels_offset" type="simple.NumberOrPercent" default="50%"/>
        <xsd:attribute name="overlap_mode" type="enums.LabelsOverlapMode" default="noOverlap"/>
        <xsd:attribute name="labels_display_mode" type="enums.LabelsDisplayMode" default="clip"/>
        <xsd:attribute name="connector_length" type="simple.NumberOrPercent" default="20%"/>
        <xsd:attribute name="outside_labels_critical_angle" type="xsd:double" default="60"/>
        <xsd:attribute name="connector_stroke" type="xsd:string" default="black 0.3"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="hover_fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hover_stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill" default="false"/>
        <xsd:attribute name="hover_hatch_fill" type="simple.HatchFill" default="false"/>
        <xsd:attribute name="force_hover_labels" type="simple.Bool" default="false"/>
        <!-- charts.Pyramid attrs -->
        <xsd:attribute name="reversed" type="simple.Bool" default="false"/>
        <!-- charts.Sparkline elements -->
        <xsd:attribute name="clip" type="simple.Bool" default="true"/>
        <xsd:attribute name="series_type" type="enums.SparkSeriesTypes" default="line"/>
        <xsd:attribute name="point_width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="connect_missing_points" type="simple.Bool" default="false"/>
        <xsd:attribute name="last_fill" type="xsd:string"/>
        <xsd:attribute name="last_hatch_fill" type="simple.HatchFill" default="false"/>
        <xsd:attribute name="last_markers" type="simple.MarkerType"/>
        <xsd:attribute name="last_labels" type="simple.BoolOrNull" default="false"/>
        <xsd:attribute name="first_fill" type="xsd:string"/>
        <xsd:attribute name="first_hatch_fill" type="simple.HatchFill" default="false"/>
        <xsd:attribute name="first_markers" type="simple.MarkerType"/>
        <xsd:attribute name="first_labels" type="simple.BoolOrNull" default="false"/>
        <xsd:attribute name="max_fill" type="xsd:string"/>
        <xsd:attribute name="max_hatch_fill" type="simple.HatchFill" default="false"/>
        <xsd:attribute name="max_markers" type="simple.MarkerType"/>
        <xsd:attribute name="max_labels" type="simple.BoolOrNull" default="false"/>
        <xsd:attribute name="min_fill" type="xsd:string"/>
        <xsd:attribute name="min_hatch_fill" type="simple.HatchFill" default="false"/>
        <xsd:attribute name="min_markers" type="simple.MarkerType"/>
        <xsd:attribute name="min_labels" type="simple.BoolOrNull" default="false"/>
        <xsd:attribute name="negative_fill" type="xsd:string"/>
        <xsd:attribute name="negative_hatch_fill" type="simple.HatchFill" default="false"/>
        <xsd:attribute name="negative_markers" type="simple.MarkerType"/>
        <xsd:attribute name="negative_labels" type="simple.BoolOrNull" default="false"/>
        <xsd:attribute name="markers" type="simple.MarkerType"/>
        <!-- charts.HeatMap elements -->
        <xsd:attribute name="color_scale" type="simple.ChartScaleRef"/>
        <xsd:attribute name="y_zoom" type="simple.NumberOrBoolOrNull"/>
        <xsd:attribute name="y_scroller" type="simple.BoolOrNull"/>
        <!-- charts.TreeMap elements -->
        <xsd:attribute name="headers_display_mode" type="enums.LabelsDisplayMode" default="clip"/>
        <xsd:attribute name="headers" type="simple.BoolOrNull"/>
        <xsd:attribute name="hover_headers" type="simple.BoolOrNull"/>
        <xsd:attribute name="max_depth" type="xsd:unsignedInt"/>
        <xsd:attribute name="hint_depth" type="xsd:unsignedInt"/>
        <xsd:attribute name="hint_opacity" type="simple.Ratio"/>
        <xsd:attribute name="max_headers_height" type="simple.NumberOrPercent"/>
        <!-- new Cartesian attributes -->
        <xsd:attribute name="x_zoom" type="simple.NumberOrBoolOrNull"/>
        <xsd:attribute name="x_scroller" type="simple.BoolOrNull"/>
    </xsd:complexType>

    <xsd:complexType name="Gauge">
        <xsd:all>
            <!-- VisualBaseWithBounds elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="bounds" type="utils.Bounds"/>
            <!-- core.Chart elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="title" type="core.Title"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="background" type="core.Background"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="margin" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="padding" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="tooltip" type="core.TooltipOrChartTooltip"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="legend" type="core.Legend"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="credits" type="core.Credits"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="chart_labels" type="core.Label_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="animation" type="utils.Animation"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="interactivity" type="utils.Interactivity"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="context_menu" type="utils.ContextMenu"/>
            <!-- Common elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="data" type="Data"/>
            <!-- anychart.charts.CircularGauge elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="cap" type="core.gauge.Cap"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="axes" type="core.axes.Circular_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="bar_pointers" type="core.gauge.pointers.Bar_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="marker_pointers" type="core.gauge.pointers.Marker_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="needle_pointers" type="core.gauge.pointers.Needle_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="knob_pointers" type="core.gauge.pointers.Knob_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="ranges" type="core.axisMarkers.CircularRange_list"/>
        </xsd:all>
        <!-- core.Chart attrs -->
        <xsd:attributeGroup ref="attrs.VisualBase"/>
        <xsd:attributeGroup ref="attrs.Bounds"/>
        <xsd:attribute name="type" type="enums.GaugeTypes" use="required"/>
        <xsd:attribute name="container" type="xsd:string"/>
        <xsd:attribute name="title" type="simple.StringOrBoolOrNull" default="true"/>
        <xsd:attribute name="background" type="simple.StringOrBoolOrNull" default="false"/>
        <xsd:attribute name="tooltip" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="animation" type="simple.Bool" default="false"/>
        <xsd:attribute name="context_menu" type="simple.Bool" default="true"/>
        <!-- anychart.charts.CircularGauge common attrs -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="start_angle" type="xsd:double"/>
        <xsd:attribute name="sweep_angle" type="xsd:double"/>
        <xsd:attribute name="circular_padding" type="simple.NumberOrPercent"/>
        <xsd:attribute name="enclose_with_straight_line" type="simple.Bool"/>
    </xsd:complexType>

    <xsd:complexType name="Gantt">
        <xsd:all>
            <!-- VisualBaseWithBounds elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="bounds" type="utils.Bounds"/>
            <!-- core.Chart elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="title" type="core.Title"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="background" type="core.Background"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="margin" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="padding" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="tooltip" type="core.TooltipOrChartTooltip"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="legend" type="core.Legend"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="animation" type="utils.Animation"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="credits" type="core.Credits"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="chart_labels" type="core.Label_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="interactivity" type="utils.Interactivity"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="context_menu" type="utils.ContextMenu"/>
            <!-- Gantt elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="controller" type="core.gantt.Controller"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="data_grid" type="core.DataGrid"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="timeline" type="core.Timeline"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="row_hover_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="row_selected_fill" type="Fill"/>
        </xsd:all>

        <xsd:attributeGroup ref="attrs.VisualBase"/>
        <xsd:attributeGroup ref="attrs.Bounds"/>
        <xsd:attribute name="type" type="enums.GanttTypes" use="required"/>
        <xsd:attribute name="container" type="xsd:string"/>
        <xsd:attribute name="title" type="simple.StringOrBoolOrNull" default="true"/>
        <xsd:attribute name="background" type="simple.StringOrBoolOrNull" default="false"/>
        <xsd:attribute name="tooltip" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="animation" type="simple.Bool" default="false"/>
        <xsd:attribute name="context_menu" type="simple.Bool" default="true"/>
        <xsd:attribute name="editing" type="simple.Bool" default="false"/>
        <!-- Gantt attributes -->

        <xsd:attribute name="header_height" type="xsd:double"/>
        <xsd:attribute name="row_hover_fill" type="xsd:string"/>
        <xsd:attribute name="row_selected_fill" type="xsd:string"/>
        <xsd:attribute name="splitter_position" type="simple.NumberOrPercent"/>
    </xsd:complexType>

    <xsd:complexType name="Map">
        <xsd:all>
            <!-- VisualBaseWithBounds elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="bounds" type="utils.Bounds"/>
            <!-- core.Chart elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="title" type="core.Title"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="background" type="core.Background"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="margin" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="padding" type="utils.Space"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="legend" type="core.Legend"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="credits" type="core.Credits"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="chart_labels" type="core.Label_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="animation" type="utils.Animation"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="crs_animation" type="utils.Animation"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="interactivity" type="utils.MapInteractivity"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="context_menu" type="utils.ContextMenu"/>
            <!-- Common elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="data" type="Data"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="palette" type="palettes.Colors"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill_palette" type="palettes.HatchFills"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="marker_palette" type="palettes.Markers"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="tooltip" type="core.TooltipOrChartTooltip"/>
            <!-- Map elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="unbound_regions" type="utils.UnboundRegionsSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="color_range" type="core.ColorRange"/>
