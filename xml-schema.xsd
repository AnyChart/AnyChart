        <xsd:attribute name="selection_mode" type="enums.SelectionMode"/>
        <xsd:attribute name="spot_radius" type="xsd:integer"/>
        <xsd:attribute name="allow_multi_series_selection" type="xsd:boolean"/>
    </xsd:complexType>

    <xsd:complexType name="utils.MapInteractivity">
        <xsd:attribute name="hover_mode" type="enums.HoverMode"/>
        <xsd:attribute name="selection_mode" type="enums.SelectionMode"/>
        <xsd:attribute name="spot_radius" type="xsd:integer"/>
        <xsd:attribute name="allow_multi_series_selection" type="xsd:boolean"/>
        <xsd:attribute name="mouse_wheel" type="xsd:boolean"/>
        <xsd:attribute name="drag" type="xsd:boolean"/>
    </xsd:complexType>

    <xsd:complexType name="utils.Rect">
        <xsd:attribute name="left" type="xsd:double"/>
        <xsd:attribute name="top" type="xsd:double"/>
        <xsd:attribute name="width" type="xsd:double"/>
        <xsd:attribute name="height" type="xsd:double"/>
    </xsd:complexType>

    <xsd:complexType name="utils.AdjustFontSizeObj">
        <xsd:attribute name="width" type="simple.Bool" default="false"/>
        <xsd:attribute name="height" type="simple.Bool" default="false"/>
    </xsd:complexType>

    <xsd:complexType name="utils.Corners">
        <xsd:attribute name="left_top" type="xsd:double" default="0"/>
        <xsd:attribute name="right_top" type="xsd:double" default="0"/>
        <xsd:attribute name="right_bottom" type="xsd:double" default="0"/>
        <xsd:attribute name="left_bottom" type="xsd:double" default="0"/>
    </xsd:complexType>

    <xsd:complexType name="utils.Error">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="x_error_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="value_error_stroke" type="Stroke"/>
        </xsd:all>
        <xsd:attribute name="mode" type="enums.ErrorMode" default="both"/>
        <xsd:attribute name="x_error" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="x_upper_error" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="x_lower_error" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="value_error" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="value_upper_error" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="value_lower_error" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="x_error_width" type="xsd:double"/>
        <xsd:attribute name="value_error_width" type="xsd:double"/>
        <xsd:attribute name="x_error_stroke" type="xsd:string"/>
        <xsd:attribute name="value_error_stroke" type="xsd:string"/>
    </xsd:complexType>

    <xsd:complexType name="utils.LegendItemSettings">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="icon_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="icon_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="icon_hatch_fill" type="HatchFill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="icon_marker_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="icon_marker_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="text" type="simple.Text"/>
        </xsd:all>
        <xsd:attribute name="icon_fill" type="xsd:string"/>
        <xsd:attribute name="icon_stroke" type="xsd:string"/>
        <xsd:attribute name="icon_hatch_fill" type="xsd:string"/>
        <xsd:attribute name="icon_marker_fill" type="xsd:string"/>
        <xsd:attribute name="icon_marker_stroke" type="xsd:string"/>
        <xsd:attribute name="icon_marker_type" type="xsd:string"/>
        <xsd:attribute name="icon_type" type="xsd:string"/>
        <xsd:attribute name="icon_text_spacing" type="xsd:double"/>
        <xsd:attribute name="icon_enabled" type="simple.Bool"/>
        <xsd:attribute name="icon_size" type="xsd:double"/>
        <xsd:attribute name="text" type="xsd:string"/>
        <xsd:attribute name="disabled" type="simple.Bool" default="false"/>
        <xsd:attributeGroup ref="attrs.TextSettings"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="utils.UnboundRegionsSettings">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
        </xsd:all>
        <xsd:attribute name="enabled" type="xsd:boolean"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
    </xsd:complexType>

    <!-- Colors -->

    <xsd:complexType name="Fill" mixed="true">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="mode" type="utils.Rect"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="keys" type="GradientKeys"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="src" type="simple.Text"/>
        </xsd:all>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="opacity" type="simple.Ratio"/>
        <xsd:attribute name="mode" type="simple.FillMode"/>
        <xsd:attribute name="angle" type="xsd:double"/>
        <xsd:attribute name="cx" type="xsd:double"/>
        <xsd:attribute name="cy" type="xsd:double"/>
        <xsd:attribute name="fx" type="xsd:double"/>
        <xsd:attribute name="fy" type="xsd:double"/>
        <xsd:attribute name="src" type="xsd:string"/>
    </xsd:complexType>

    <xsd:complexType name="Stroke" mixed="true">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="mode" type="utils.Rect"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="keys" type="GradientKeys"/>
        </xsd:all>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="thickness" type="xsd:double" default="1"/>
        <xsd:attribute name="opacity" type="simple.Ratio" default="1"/>
        <xsd:attribute name="dash" type="xsd:string" default="none"/>
        <xsd:attribute name="line_join" type="enums.LineJoin"/>
        <xsd:attribute name="line_cap" type="enums.LineCap"/>
        <xsd:attribute name="angle" type="xsd:double"/>
        <xsd:attribute name="mode" type="simple.BoolOrNull" default="false"/>
        <xsd:attribute name="cx" type="xsd:double"/>
        <xsd:attribute name="cy" type="xsd:double"/>
        <xsd:attribute name="fx" type="xsd:double"/>
        <xsd:attribute name="fy" type="xsd:double"/>
    </xsd:complexType>

    <xsd:complexType name="GradientKey">
        <xsd:complexContent>
            <xsd:extension base="simple.Text">
                <xsd:attribute name="offset" type="simple.Ratio"/>
                <xsd:attribute name="color" type="xsd:string"/>
                <xsd:attribute name="opacity" type="simple.Ratio"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="GradientKeys">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="key" type="GradientKey"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="HatchFill">
        <xsd:complexContent>
            <xsd:extension base="HatchFillString">
                <xsd:attribute name="type" type="enums.HatchFill"/>
                <xsd:attribute name="color" type="xsd:string"/>
                <xsd:attribute name="thickness" type="xsd:double" default="1"/>
                <xsd:attribute name="size" type="xsd:double"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="HatchFillString">
        <xsd:simpleContent>
            <xsd:restriction base="simple.Text">
                <xsd:enumeration value=""/>
                <xsd:enumeration value="true"/>
                <xsd:enumeration value="false"/>
                <xsd:enumeration value="null"/>
                <xsd:enumeration value="none"/>
                <xsd:enumeration value="backwardDiagonal"/>
                <xsd:enumeration value="forwardDiagonal"/>
                <xsd:enumeration value="horizontal"/>
                <xsd:enumeration value="vertical"/>
                <xsd:enumeration value="dashedBackwardDiagonal"/>
                <xsd:enumeration value="grid"/>
                <xsd:enumeration value="dashedForwardDiagonal"/>
                <xsd:enumeration value="dashedHorizontal"/>
                <xsd:enumeration value="dashedVertical"/>
                <xsd:enumeration value="diagonalCross"/>
                <xsd:enumeration value="diagonalBrick"/>
                <xsd:enumeration value="divot"/>
                <xsd:enumeration value="horizontalBrick"/>
                <xsd:enumeration value="verticalBrick"/>
                <xsd:enumeration value="checkerBoard"/>
                <xsd:enumeration value="confetti"/>
                <xsd:enumeration value="plaid"/>
                <xsd:enumeration value="solidDiamond"/>
                <xsd:enumeration value="zigZag"/>
                <xsd:enumeration value="weave"/>
                <xsd:enumeration value="percent05"/>
                <xsd:enumeration value="percent10"/>
                <xsd:enumeration value="percent20"/>
                <xsd:enumeration value="percent25"/>
                <xsd:enumeration value="percent30"/>
                <xsd:enumeration value="percent40"/>
                <xsd:enumeration value="percent50"/>
                <xsd:enumeration value="percent60"/>
                <xsd:enumeration value="percent70"/>
                <xsd:enumeration value="percent75"/>
                <xsd:enumeration value="percent80"/>
                <xsd:enumeration value="percent90"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <!-- Scales -->

    <xsd:complexType name="Scale">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="ticks" type="ScaleTicks"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="minor_ticks" type="ScaleTicks"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="values" type="OrdinalScaleValues"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="names" type="OrdinalScaleNames"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="ranges" type="OrdinalColorScaleRanges"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="colors" type="OrdinalOrLinearScaleColors"/>
        </xsd:all>
        <xsd:attribute name="type" type="enums.ScaleTypes" default="linear"/>
        <xsd:attribute name="inverted" type="simple.Bool" default="false"/>
        <xsd:attribute name="stick_to_zero" type="simple.Bool"/>
        <xsd:attribute name="stack_mode" type="enums.ScaleStackMode" default="none"/>
        <xsd:attribute name="log_base" type="xsd:double" default="10"/>
        <xsd:attribute name="names" type="xsd:string" default="name"/>
        <xsd:attribute name="minimum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="maximum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="soft_minimum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="soft_maximum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="minimum_gap" type="xsd:double" default="0.1"/>
        <xsd:attribute name="maximum_gap" type="xsd:double" default="0.1"/>
    </xsd:complexType>

    <xsd:complexType name="ColorScale">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="ticks" type="ScaleTicks"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="minor_ticks" type="ScaleTicks"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="colors" type="OrdinalColorScaleColors"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="ranges" type="OrdinalColorScaleRanges"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="names" type="OrdinalScaleNames"/>
        </xsd:all>
        <xsd:attribute name="type" type="enums.ScaleTypes" default="ordinalColor"/>
        <xsd:attribute name="inverted" type="simple.Bool" default="false"/>
        <xsd:attribute name="names" type="xsd:string" default="name"/>
    </xsd:complexType>

    <xsd:complexType name="OrdinalColorScale">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="ticks" type="ScaleTicks"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="colors" type="OrdinalColorScaleColors"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="ranges" type="OrdinalColorScaleRanges"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="names" type="OrdinalScaleNames"/>
        </xsd:all>
        <xsd:attribute name="type" type="enums.ScaleTypes" default="ordinalColor"/>
        <xsd:attribute name="inverted" type="simple.Bool" default="false"/>
        <xsd:attribute name="names" type="xsd:string" default="name"/>
    </xsd:complexType>

    <xsd:complexType name="GeoScale">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="bounds" type="utils.Rect"/>
        </xsd:all>
        <xsd:attribute name="type" type="enums.ScaleTypes" default="geo"/>
        <xsd:attribute name="minimum_x" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="minimum_y" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="maximum_x" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="maximum_y" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="inverted_x" type="simple.Bool"/>
        <xsd:attribute name="inverted_y" type="simple.Bool"/>
        <xsd:attribute name="gap" type="xsd:double"/>
    </xsd:complexType>

    <xsd:complexType name="LogOrLinScale">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="ticks" type="ScaleTicks"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="minor_ticks" type="ScaleTicks"/>
        </xsd:all>
        <xsd:attribute name="type" type="enums.GaugeScaleTypes" default="linear"/>
        <xsd:attribute name="inverted" type="simple.Bool" default="false"/>
        <xsd:attribute name="stick_to_zero" type="simple.Bool"/>
        <xsd:attribute name="stack_mode" type="enums.ScaleStackMode" default="none"/>
        <xsd:attribute name="log_base" type="xsd:double" default="10"/>
        <xsd:attribute name="minimum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="maximum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="soft_minimum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="soft_maximum" type="simple.DoubleOrNull" default="null"/>
        <xsd:attribute name="minimum_gap" type="xsd:double" default="0.1"/>
        <xsd:attribute name="maximum_gap" type="xsd:double" default="0.1"/>
    </xsd:complexType>

    <xsd:complexType name="Scale_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="scale" type="Scale"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ColorScale_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="scale" type="ColorScale"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="ScaleTicks">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="explicit" type="ScaleTicksValues"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="names" type="OrdinalScaleNames"/>
        </xsd:all>
        <xsd:attribute name="mode" type="enums.ScatterTicksMode"/>
        <xsd:attribute name="base" type="xsd:double" default="0"/>
        <xsd:attribute name="count" type="xsd:unsignedInt"/>
        <xsd:attribute name="min_count" type="xsd:unsignedInt" default="4"/>
        <xsd:attribute name="max_count" type="xsd:unsignedInt" default="6"/>
        <xsd:attribute name="interval" type="simple.ScaleInterval"/>
    </xsd:complexType>

    <xsd:complexType name="OrdinalScaleValues">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="value" type="simple.Text"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalScaleNames">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="name" type="simple.Text"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalColorScaleColors">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="color" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalOrLinearScaleColors">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="color" type="LinearColors"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalColorScaleRanges">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="range" type="OrdinalColorRange"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalColorRange">
        <xsd:attribute name="less" type="xsd:unsignedInt"/>
        <xsd:attribute name="greater" type="xsd:unsignedInt"/>
        <xsd:attribute name="from" type="xsd:unsignedInt"/>
        <xsd:attribute name="to" type="xsd:unsignedInt"/>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="name" type="xsd:string"/>
    </xsd:complexType>

    <xsd:complexType name="LinearColors">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="color" type="simple.Text"/>
        </xsd:sequence>
        <xsd:attribute name="offset" type="xsd:double"/>
    </xsd:complexType>

    <xsd:complexType name="ScaleTicksValues">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="tick" type="simple.Text"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GanttDateTimeScale">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="range" type="simple.MinMaxRange"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="total_range" type="simple.MinMaxRange"/>
        </xsd:all>
    </xsd:complexType>

    <!-- Gauge elements -->

    <xsd:complexType name="core.gauge.Cap">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="radius" type="simple.NumberOrPercent"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Bar">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Bar pointer attr -->
        <xsd:attribute name="radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="position" type="enums.GaugeSidePosition"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Bar_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Bar"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Knob">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Knob pointer attr -->
        <xsd:attribute name="vertices_count" type="xsd:double"/>
        <xsd:attribute name="vertices_curvature" type="xsd:double"/>
        <xsd:attribute name="top_ratio" type="xsd:double"/>
        <xsd:attribute name="bottom_ratio" type="xsd:double"/>
        <xsd:attribute name="top_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="bottom_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Knob_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Knob"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Marker">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Bar pointer attr -->
        <xsd:attribute name="radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="size" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="position" type="enums.GaugeSidePosition"/>
        <xsd:attribute name="type" type="enums.MarkerType"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Marker_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Marker"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Needle">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Knob pointer attr -->
        <xsd:attribute name="start_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="middle_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="end_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="start_width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="middle_width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="end_width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Needle_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Needle"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Gantt elements -->
    <xsd:complexType name="core.gantt.Controller">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="tree_data" type="TreeData"/>
        </xsd:all>
        <xsd:attribute name="is_resource_chart" type="simple.Bool" default="false"/>
        <xsd:attribute name="vertical_offset" type="xsd:double" default="0"/>
        <xsd:attribute name="start_index" type="xsd:double" default="0"/>
    </xsd:complexType>


    <!-- Cartesian zoom -->
    <xsd:complexType name="core.CartesianXZoom">
        <xsd:attribute name="continuous" type="simple.Bool" default="true"/>
        <xsd:attribute name="start_ratio" type="simple.Ratio" default="0"/>
        <xsd:attribute name="end_ratio" type="simple.Ratio" default="1"/>
        <xsd:attribute name="points_count" type="xsd:unsignedInt"/>
        <xsd:attribute name="start_value" type="xsd:string"/>
        <xsd:attribute name="end_value" type="xsd:string"/>
        <xsd:attribute name="scale" type="simple.ScaleRef"/>
    </xsd:complexType>

    <!-- Chart scroller -->
    <xsd:complexType name="core.ChartScroller">
        <xsd:all>
            <xsd:element name="fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="selected_fill" type="Fill" minOccurs="0" maxOccurs="1"/>
