            <xsd:element minOccurs="0" maxOccurs="1" name="color_range" type="core.ColorRange"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="geo_scale" type="GeoScale"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="series_list" type="Series_list"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="color_scales" type="ColorScale_list"/>

            <xsd:element minOccurs="0" maxOccurs="1" name="crosshair" type="core.Crosshair"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="axes_settings" type="core.axes.MapSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="grids_settings" type="core.grids.MapSettings"/>

            <xsd:element minOccurs="0" maxOccurs="1" name="geo_data" type="xsd:anyType"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="drill_down_map" type="DrillDownMap"/>
