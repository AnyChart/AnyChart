    <xsd:complexType name="OrdinalColorScaleColors">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="color" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalOrLinearScaleColors">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="color" type="simple.Text"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalColorScaleRanges">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="range" type="OrdinalColorRange"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OrdinalColorRange">
        <xsd:attribute name="less" type="xsd:unsignedInt"/>
        <xsd:attribute name="greater" type="xsd:unsignedInt"/>
        <xsd:attribute name="from" type="xsd:unsignedInt"/>
        <xsd:attribute name="to" type="xsd:unsignedInt"/>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="name" type="xsd:string"/>
    </xsd:complexType>

    <xsd:complexType name="ScaleTicksValues">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="tick" type="simple.Text"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GanttDateTimeScale">
        <xsd:attribute name="visible_minimum" type="xsd:double"/>
        <xsd:attribute name="visible_maximum" type="xsd:double"/>
        <xsd:attribute name="soft_minimum" type="xsd:double"/>
        <xsd:attribute name="soft_maximum" type="xsd:double"/>
        <xsd:attribute name="data_minimum" type="xsd:double"/>
        <xsd:attribute name="data_maximum" type="xsd:double"/>
        <xsd:attribute name="minimum" type="xsd:double"/>
        <xsd:attribute name="maximum" type="xsd:double"/>
        <xsd:attribute name="minimum_gap" type="xsd:double"/>
        <xsd:attribute name="maximum_gap" type="xsd:double"/>
    </xsd:complexType>

    <xsd:complexType name="TimelineHeader">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="background_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="levels_separation_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="top_level" type="TimelineHeaderLevel"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="mid_level" type="TimelineHeaderLevel"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="low_level" type="TimelineHeaderLevel"/>
        </xsd:all>
        <xsd:attribute name="background_fill" type="xsd:string"/>
        <xsd:attribute name="levels_separation_stroke" type="xsd:string"/>

        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>


    <xsd:complexType name="TimelineHeaderLevel">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="tile_fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="tiles_separation_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="labels" type="core.LabelsFactory" />
        </xsd:all>
        <xsd:attribute name="tile_fill" type="xsd:string"/>
        <xsd:attribute name="tiles_separation_stroke" type="xsd:string"/>

        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <!-- Gauge elements -->

    <xsd:complexType name="core.gauge.Cap">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="radius" type="simple.NumberOrPercent"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Bar">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Bar pointer attr -->
        <xsd:attribute name="radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="position" type="enums.GaugeSidePosition"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Bar_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Bar"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Knob">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Knob pointer attr -->
        <xsd:attribute name="vertices_count" type="xsd:double"/>
        <xsd:attribute name="vertices_curvature" type="xsd:double"/>
        <xsd:attribute name="top_ratio" type="xsd:double"/>
        <xsd:attribute name="bottom_ratio" type="xsd:double"/>
        <xsd:attribute name="top_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="bottom_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Knob_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Knob"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Marker">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Bar pointer attr -->
        <xsd:attribute name="radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="size" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="position" type="enums.GaugeSidePosition"/>
        <xsd:attribute name="type" type="enums.MarkerType"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Marker_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Marker"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Needle">
        <xsd:all>
            <!-- Pointers base elements -->
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill"/>
        </xsd:all>
        <!-- Pointers base attr -->
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="axis_index" type="xsd:double"/>
        <xsd:attribute name="data_index" type="xsd:double"/>
        <!-- Knob pointer attr -->
        <xsd:attribute name="start_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="middle_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="end_radius" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="start_width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="middle_width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="end_width" type="simple.NumberOrPercentOrNull"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.gauge.pointers.Needle_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.gauge.pointers.Needle"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.linearGauge.pointers.Pointer">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="color" type="Fill" />
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill" />
            <xsd:element minOccurs="0" maxOccurs="1" name="empty_fill" type="Fill" />
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke" />
            <xsd:element minOccurs="0" maxOccurs="1" name="hatch_fill" type="HatchFill" />
            <xsd:element minOccurs="0" maxOccurs="1" name="empty_hatch_fill" type="HatchFill" />
            <xsd:element minOccurs="0" maxOccurs="1" name="legend_item" type="utils.LegendItemSettings" />
            <xsd:element minOccurs="0" maxOccurs="1" name="label" type="core.LabelsFactory" />
            <xsd:element minOccurs="0" maxOccurs="1" name="color_scale" type="LinearOrdinalColorScale" />
            <xsd:element minOccurs="0" maxOccurs="1" name="scale" type="Scale" />
            <xsd:element minOccurs="0" maxOccurs="1" name="normal" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hovered" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="selected" type="core.StateSettings"/>
        </xsd:all>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="empty_fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="empty_hatch_fill" type="simple.HatchFill"/>
        <xsd:attribute name="pointer_type" type="enums.LinearGaugePointerType" />
        <xsd:attribute name="id" type="xsd:string"/>
        <xsd:attribute name="auto_index" type="xsd:unsignedInt"/>
        <xsd:attribute name="data_index" type="xsd:unsignedInt"/>
        <xsd:attribute name="name" use="optional" type="xsd:string" />
        <xsd:attribute name="width" type="xsd:string" />
        <xsd:attribute name="offset" type="xsd:string" />
        <xsd:attribute name="selection_mode" type="enums.SelectionMode" />
        <!--Marker pointer-->
        <xsd:attribute name="type" type="enums.MarkerType" />
        <!--Tank pointer-->
        <xsd:attribute name="bulb_radius" type="xsd:string" />
        <xsd:attribute name="bulb_padding" type="xsd:string" />
        <!--Led pointer-->
        <xsd:attribute name="gap" type="simple.NumberOrPercentOrNull" />
        <xsd:attribute name="size" type="simple.NumberOrPercentOrNull" />
        <xsd:attribute name="count" type="simple.NumberOrNull" />
        <xsd:attribute name="scale" type="simple.ScaleRef"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.linearGauge.pointers.Pointer_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="pointer" type="core.linearGauge.pointers.Pointer"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.linearGauge.ScaleBar.ControlPoints">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="point">
                <xsd:complexType>
                    <xsd:attribute name="height" type="simple.Ratio"/>
                    <xsd:attribute name="left" type="simple.Ratio"/>
                    <xsd:attribute name="right" type="simple.Ratio"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="core.linearGauge.ScaleBar">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill" />
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke" />
            <xsd:element minOccurs="0" maxOccurs="1" name="color_scale" type="LinearOrdinalColorScale" />
            <xsd:element minOccurs="0" maxOccurs="1" name="scale" type="Scale" />
            <xsd:element minOccurs="0" maxOccurs="1" name="points" type="core.linearGauge.ScaleBar.ControlPoints"/>
        </xsd:all>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="width" type="xsd:string" />
        <xsd:attribute name="offset" type="xsd:string" />
        <xsd:attribute name="from" type="xsd:string" />
        <xsd:attribute name="to" type="xsd:string" />
        <xsd:attribute name="scale" type="simple.ScaleRef"/>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
    </xsd:complexType>

    <xsd:complexType name="core.linearGauge.ScaleBar_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="scale_bar" type="core.linearGauge.ScaleBar"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Gantt elements -->
    <xsd:complexType name="core.gantt.Controller">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="tree_data" type="TreeData"/>
        </xsd:all>
        <xsd:attribute name="is_resource_chart" type="simple.Bool" default="false"/>
        <xsd:attribute name="vertical_offset" type="xsd:double" default="0"/>
        <xsd:attribute name="start_index" type="xsd:double" default="0"/>
    </xsd:complexType>


    <!-- Cartesian zoom -->
    <xsd:complexType name="core.CartesianXZoom">
        <xsd:attribute name="continuous" type="simple.Bool" default="true"/>
        <xsd:attribute name="start_ratio" type="simple.Ratio" default="0"/>
        <xsd:attribute name="end_ratio" type="simple.Ratio" default="1"/>
        <xsd:attribute name="points_count" type="xsd:unsignedInt"/>
        <xsd:attribute name="start_value" type="xsd:string"/>
        <xsd:attribute name="end_value" type="xsd:string"/>
        <xsd:attribute name="scale" type="simple.ScaleRef"/>
    </xsd:complexType>

    <!-- Chart scroller -->
    <xsd:complexType name="core.ChartScroller">
        <xsd:all>
            <xsd:element name="fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="selected_fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="outline_stroke" type="Stroke" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="thumbs" type="core.ScrollerThumbs" minOccurs="0" maxOccurs="1"/>
        </xsd:all>
        <xsd:attributeGroup ref="attrs.VisualBase"/>
        <xsd:attribute name="orientation" type="enums.Orientation"/>
        <xsd:attribute name="inverted" type="simple.Bool"/>
        <xsd:attribute name="auto_hide" type="simple.Bool" default="false"/>
        <xsd:attribute name="allow_range_change" type="simple.Bool" default="true"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="selected_fill" type="xsd:string"/>
        <xsd:attribute name="outline_stroke" type="xsd:string"/>
        <xsd:attribute name="height" type="simple.NumberOrPercent"/>
        <xsd:attribute name="min_height" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="max_height" type="simple.NumberOrPercentOrNull"/>
        <xsd:attribute name="thumbs" type="simple.BoolOrNull"/>
        <xsd:attribute name="position" type="enums.ChartScrollerPosition" default="after-axes"/>
    </xsd:complexType>

    <xsd:simpleType name="enums.ChartScrollerPosition">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="before-axes"/>
            <xsd:enumeration value="after-axes"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="core.ScrollerThumbs">
        <xsd:all>
            <xsd:element name="fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="stroke" type="Stroke" minOccurs="0" maxOccurs="1"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="normal" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hovered" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="selected" type="core.StateSettings"/>
        </xsd:all>
        <xsd:attribute name="enabled" type="simple.BoolOrNull"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="auto_hide" type="simple.Bool" default="false"/>
    </xsd:complexType>

    <xsd:complexType name="core.Milestones">
        <xsd:all>
            <xsd:element name="color" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="stroke" type="Stroke" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="labels" type="core.LabelsFactory" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="tooltip" type="core.Tooltip" minOccurs="0" maxOccurs="1"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="normal" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hovered" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="selected" type="core.StateSettings"/>
        </xsd:all>
        <xsd:attribute name="enabled" type="simple.BoolOrNull"/>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="labels" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="shape" type="xsd:string" default="circle"/>
        <xsd:attribute name="size" type="simple.NumberOrPercent"/>
        <xsd:attribute name="tooltip" type="simple.BoolOrNull" default="true"/>
    </xsd:complexType>

    <xsd:complexType name="core.Tasks">
        <xsd:all>
            <xsd:element name="color" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="stroke" type="Stroke" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="dummy_fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="dummy_stroke" type="Stroke" minOccurs="0" maxOccurs="1"/>

            <xsd:element name="upper_labels" type="core.LabelsFactory" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="lower_labels" type="core.LabelsFactory" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="tooltip" type="core.Tooltip" minOccurs="0" maxOccurs="1"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="normal" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hovered" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="selected" type="core.StateSettings"/>
        </xsd:all>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="enabled" type="simple.BoolOrNull"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="dummy_fill" type="xsd:string"/>
        <xsd:attribute name="dummy_stroke" type="xsd:string"/>
        <xsd:attribute name="upper_labels" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="lower_labels" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="tooltip" type="simple.BoolOrNull" default="true"/>
    </xsd:complexType>


    <xsd:complexType name="core.Intersections">
        <xsd:all>
            <xsd:element name="fill" type="Fill" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="stroke" type="Stroke" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="hatch_fill" minOccurs="0" maxOccurs="1" type="HatchFill"/>

            <xsd:element name="labels" type="core.LabelsFactory" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="markers" type="core.MarkersFactory" minOccurs="0" maxOccurs="1"/>

            <xsd:element name="tooltip" type="core.Tooltip" minOccurs="0" maxOccurs="1"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="normal" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="hovered" type="core.StateSettings"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="selected" type="core.StateSettings"/>
        </xsd:all>
        <xsd:attribute name="enabled" type="simple.BoolOrNull"/>
        <xsd:attribute name="fill" type="xsd:string"/>
        <xsd:attribute name="stroke" type="xsd:string"/>
        <xsd:attribute name="hatch_fill" type="xsd:string"/>

        <xsd:attribute name="labels" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="markers" type="simple.BoolOrNull" default="true"/>
        <xsd:attribute name="tooltip" type="simple.BoolOrNull" default="true"/>
    </xsd:complexType>

    <xsd:complexType name="core.CriticalPath">
        <xsd:all>
            <xsd:element name="tasks" type="core.Tasks" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="milestones" type="core.Milestones" minOccurs="0" maxOccurs="1"/>
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="core.quadrant.Quarter">
        <xsd:all>
            <xsd:element minOccurs="0" maxOccurs="1" name="fill" type="Fill" />
            <xsd:element minOccurs="0" maxOccurs="1" name="stroke" type="Stroke" />
            <xsd:element minOccurs="0" maxOccurs="1" name="top_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="right_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="bottom_stroke" type="Stroke"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="left_stroke" type="Stroke"/>
