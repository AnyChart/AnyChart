        <xsd:attribute name="axes_settings" type="simple.BoolOrNull"/>
        <xsd:attribute name="grids_settings" type="simple.BoolOrNull"/>
        <xsd:attribute name="overlap_mode" type="simple.StringOrBoolOrNull"/>
