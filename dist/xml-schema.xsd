    <xsd:complexType name="LinearOrdinalColorScaleColors">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="color" type="xsd:anyType" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Scale_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
