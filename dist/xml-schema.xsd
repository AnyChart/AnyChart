            <!-- factories -->
            <xsd:element minOccurs="0" maxOccurs="1" name="labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="outlier_markers" type="core.MarkersFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="headers" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="upper_labels" type="core.LabelsFactory"/>
            <xsd:element minOccurs="0" maxOccurs="1" name="lower_labels" type="core.LabelsFactory"/>
        </xsd:all>
