        <!-- Connector series -->
        <xsd:attribute name="end_size" type="xsd:double"/>
        <xsd:attribute name="start_size" type="xsd:double"/>
        <xsd:attribute name="curvature" type="xsd:double"/>
        <!-- Choropleth series -->
        <xsd:attribute name="color_scale" type="simple.ScaleRef"/>
    </xsd:complexType>

    <xsd:complexType name="Series_list">
        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
